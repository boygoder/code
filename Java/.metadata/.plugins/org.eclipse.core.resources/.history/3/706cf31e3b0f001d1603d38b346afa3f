package property;

public class Item {
		String name_ = "长剑";
		int price_ = 200;
		
		public void buy()
		{
			System.out.println("购买了"+this.name_);
		}
		public void effect()
		{
			System.out.println("效果：未知");
		}
		public Item() {};
		public  Item(String name,int price)
		{
			name_ = name;
			price_ = price;
		}
		public static void main(String[] args) {
			Item blood = new Item();
			blood.name_="血瓶";
			blood.price_=50;
			
			Item shoes = new Item();
			shoes.name_="草鞋";
			shoes.price_=300;
			
			Item sword = new Item();
			sword.name_="长剑";
			sword.price_=350;
		}
}
