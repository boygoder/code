package com.ClassAndObject;

public class Item {
		String name_ = "长剑";
		int price_ = 200;
		public  Item(String name,int price): name_(name)
		{
			name_ = name;
			price_ = price;
		}
		public static void main(String[] args) {
			Item blood = new Item();
			blood.name="血瓶";
			blood.price=50;
			
			Item shoes = new Item();
			shoes.name="草鞋";
			shoes.price=300;
			
			Item sword = new Item();
			sword.name="长剑";
			sword.price=350;
		}
}
